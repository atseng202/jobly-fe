);\n\n      return invalidInputArr.length > 0;\n    }\n\n  \n  return (\n    <form onSubmit={handleSubmit}>\n      <label htmlFor=\"username\">Username</label>\n      <input \n          name=\"username\" \n          id=\"username\"\n          value={formData.username}\n          onChange={handleChange}\n          required \n        />\n      <label htmlFor=\"password\">Password</label>\n      <input \n          name=\"password\" \n          id=\"password\"\n          value={formData.password}\n          type=\"password\"\n          onChange={handleChange}\n          required  \n        />\n      <label htmlFor=\"firstName\">First Name</label>\n      <input \n          name=\"firstName\" \n          id=\"firstName\"\n          value={formData.firstName}\n          onChange={handleChange}\n          required  \n        />\n      <label htmlFor=\"lastName\">Last Name</label>\n      <input \n          name=\"lastName\" \n          id=\"lastName\"\n          value={formData.lastName}\n          onChange={handleChange}\n          required  \n        />\n      <label htmlFor=\"email\">Email</label>\n      <input \n          name=\"email\"\n          type=\"email\" \n          id=\"email\"\n          value={formData.email}\n          onChange={handleChange}\n          required  \n        />\n      <button disabled={notDone()} >Submit</button>\n    </form>\n  )\n}\n\nexport default SignupForm;","/Users/atseng202/Desktop/Programming Master Folder/rithm_school_master/curriculum/week-9/react-jobly/src/JobCardList.js",[],"/Users/atseng202/Desktop/Programming Master Folder/rithm_school_master/curriculum/week-9/react-jobly/src/SearchForm.js",[],"/Users/atseng202/Desktop/Programming Master Folder/rithm_school_master/curriculum/week-9/react-jobly/src/CompanyList.js",[],"/Users/atseng202/Desktop/Programming Master Folder/rithm_school_master/curriculum/week-9/react-jobly/src/JobCard.js",[],"/Users/atseng202/Desktop/Programming Master Folder/rithm_school_master/curriculum/week-9/react-